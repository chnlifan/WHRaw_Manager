<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
      <add key="ShowRackName" value="False"/>
      <add key="ShowRawnameInRack" value="False"/>
      <add key="RackMarkBackColor" value="Red"/>
      <add key="RackMarkForeColor" value="Yellow"/>
      <add key="QueryRackMarkBackColor" value="Yellow"/>
      <add key="QueryRackMarkForeColor" value="Red"/>
	  <add key="ReceiveRule" value="off"/>
	  <add key="SingleShowMaxRowNum" value="10"/>
	  <add key="Ware2color" value="#BFCDDB"/>
  </appSettings>
  
  <connectionStrings>
    <add name="SQLConnStr" connectionString="Data Source=.\SQLEXPRESS;initial catalog=CCAWHRaw;User Id=MyUser;Password=MyPwd" providerName="System.Data.SqlClient"/>
  </connectionStrings>
</configuration>